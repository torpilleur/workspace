        <Slider x:Name="slider" Height="51.833" Margin="29.389,148.917,0,0" VerticalAlignment="Top" Style="{DynamicResource SliderStyle1}" HorizontalAlignment="Left" Width="88.377" MouseLeftButtonDown="slider_MouseLeftButtonDown" ValueChanged="slider_ValueChanged"/>
        <TextBlock x:Name="valeur_slider" Height="19" Margin="129.516,154,255.146,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="Valeur d'essai">
            <TextBlock.Foreground>
                <LinearGradientBrush EndPoint="0.5,1" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
                    <GradientStop Color="#FFC6E8EE" Offset="1"/>
                    <GradientStop Color="#FF18FBDC"/>
